{
  "compilerOptions": {
    "outDir": "dist",
    "lib": [
      "dom",
      "es2017.object"
    ],
    "target": "es5",
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "module": "esnext",
    "noEmitOnError": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}
